{
    "manifest_version": 3,
    "name": "Keyword Blocker",
    "version": "1.0",
    "permissions": ["declarativeContent", "contextMenus", "activeTab"],
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "16": "logo.png",
      "48": "logo.png",
      "128": "logo.png"
    },
    "action": {
      "default_icon": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
      }
    }
  }
  